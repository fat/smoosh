{
  "VERSION": "0.0.1",
  "JSHINT_OPTS": { "boss": true, "forin": true, "browser": true },
  "JAVASCRIPT": {
    "DIST_DIR": "dist",
    "base": [ "./src/header.js", "./src/script.js" ],
    "secondary": [ "./src/foo.js", "./src/foo.bar.js", "./src/foo.bar.baz.js" ]
  },
   "UGLIFYJS_OPTS": {
   	"mangle": true, 
   	"squeeze": true, 
   	"mangle_options": { 
   		"except" : ["ExampleA", "Example B", "Example C"] 
   		}, 
   	"squeeze_options": {
   		"make_seqs": false 
   		}, 
   	"gen_code_options": {
   	"beautify": true 
   	} 
   },
  "CSS": {
    "DIST_DIR": "dist",
    "styles": ["./src/bar.css", "./src/foo.css"]
  }
}